  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug CLI",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/packages/cli/src/index.ts",
      "args": [
        "pull",
        "-n",
        "${env:NOTION_DATABASE_ID}",
        "-r",
        "${env:NOTION_DATABASE_ID}",
        "--log-level",
        "debug"
      ],
      //   "cwd": "${workspaceFolder}/packages/cli/",
      "outFiles": ["${workspaceFolder}/packages/cli/dist/**/*.js"]
    }
  ]
}
